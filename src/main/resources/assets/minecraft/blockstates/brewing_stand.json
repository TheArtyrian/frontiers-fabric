{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/brewing_stand"
      },
      "when": {
        "frontiers_has_rod": "false"
      }
    },
    {
      "apply": {
        "model": "frontiers:block/brewing_stand_rodded"
      },
      "when": {
        "frontiers_has_rod": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_bottle0"
      },
      "when": {
        "has_bottle_0": "true",
        "frontiers_lightning_0": "false"
      }
    },
    {
      "apply": {
        "model": "frontiers:block/brewing_stand_lightning0"
      },
      "when": {
        "has_bottle_0": "true",
        "frontiers_lightning_0": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_bottle1"
      },
      "when": {
        "has_bottle_1": "true",
        "frontiers_lightning_1": "false"
      }
    },
    {
      "apply": {
        "model": "frontiers:block/brewing_stand_lightning1"
      },
      "when": {
        "has_bottle_1": "true",
        "frontiers_lightning_1": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_bottle2"
      },
      "when": {
        "has_bottle_2": "true",
        "frontiers_lightning_2": "false"
      }
    },
    {
      "apply": {
        "model": "frontiers:block/brewing_stand_lightning2"
      },
      "when": {
        "has_bottle_2": "true",
        "frontiers_lightning_2": "true"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_empty0"
      },
      "when": {
        "has_bottle_0": "false"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_empty1"
      },
      "when": {
        "has_bottle_1": "false"
      }
    },
    {
      "apply": {
        "model": "minecraft:block/brewing_stand_empty2"
      },
      "when": {
        "has_bottle_2": "false"
      }
    }
  ]
}